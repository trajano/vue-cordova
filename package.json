{
  "name": "vue-cordova",
  "version": "0.1.5",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fmt": "vue-cli-service lint --fix",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test": "vue-cli-service test:unit && vue-cli-service test:e2e --headless",
    "version": "sync-cordova-xml package.json config.xml --output=config.xml && git add config.xml"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.4",
    "@vue/cli-plugin-e2e-cypress": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-plugin-unit-mocha": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "chai": "^4.1.2",
    "cordova-android": "^8.1.0",
    "cordova-electron": "^1.1.1",
    "cordova-plugin-whitelist": "^1.3.4",
    "cypress": "^3.8.3",
    "eslint": "^6.8.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "sync-cordova-xml": "^0.4.0",
    "typescript": "^3.7.5",
    "vue-template-compiler": "^2.6.11"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "electron",
      "android"
    ]
  }
}
